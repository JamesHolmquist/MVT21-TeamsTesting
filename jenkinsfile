pipeline {

      agent any

    tools {nodejs "nodejs"}

    stages {
        stage('Build') {
            steps {
                // Get some code from a GitHub repository
                git branch: 'main',
                url: 'https://github.com/JamesHolmquist/MVT21-TeamsTesting.git'
            }
        }
        stage('Run Tests') {
            steps {
                sh 'npm i'
                sh 'npm install cypress'
                sh 'chmod -R +rwx *'
                sh 'npx cypress run'
            }    
        }
    }
}
